{
  "name": "@ant-design-vue/pro-template",
  "version": "0.1.0",
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "clean": "rimraf -rf packages/*/{node_modules,dist,es,lib} && rimraf node_modules",
    "commit": "git-cz",
    "start:all": "npm-run-all --parallel start:host start:apps",
    "start:apps": "pnpm --filter apps start",
    "start": "pnpm --filter host start"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
